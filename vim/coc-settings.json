{
    "suggest.triggerCompletionWait": 50,
    "suggest.minTriggerInputLength": 2,
    "languageserver": {
        "racket": {
            "command": "racket",
            "args": [
                "--lib",
                "racket-langserver"
            ],
            "filetypes": [
                "scheme",
                "racket"
            ]
        },
        "digestif": {
            "command": "digestif",
            "filetypes": ["tex", "plaintex", "context"]
        }
    },
    // "markdownlint.config": {
    //     "default": true,
    //     "MD024": {
    //         "siblings_only": true
    //     }
    // },
    "clojure.enable": true,
    // "clojure.executable": "~/Personal/clojure-lsp/clojure-lsp",
    "clojure.initialization-options.copy-kond-configs?": true,
    "clojure.initialization-options.log-path": "/tmp/clojure-lsp.out",
    "clojure.initialization-options.semantic-tokens?": true,
    "clojure.initialization-options.clean": {
        "automatically-after-ns-refactor": false
    },
    "clojure.initialization-options.hover": {
        "clojuredocs": true,
        "arity-on-same-line?": false
    },
    // "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/13.0.0/clangd_13.0.0/bin/clangd",
    "clojure.startup-message": true,
    "clojure.trace.server": "verbose",
    "coc.preferences.enableMarkdown": true,
    "suggest.triggerCompletionWait": 50,
    // "suggest.minTriggerInputLength": 2,
    "coc.preferences.excludeImageLinksInMarkdownDocument": true,
    "coc.preferences.renameFillCurrent": false,
    "codeLens.enable": true,
    "codeLens.position": "eol",
    "codeLens.separator": "->",
    "diagnostic.displayByAle": false,
    "javascript.suggestionActions.enabled": false,
    // "jedi.startupMessage": true,
    // "jedi.markupKindPreferred": "markdown",
    // "jedi.trace.server": "verbose",
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.cargo.autoreload": true,
    "rust-analyzer.checkOnSave.enable": true,
    "rust-analyzer.completion.autoimport.enable": true,
    "rust-analyzer.inlayHints.chainingHints": false,
    "rust-analyzer.inlayHints.chainingHintsSeparator": "‣ ",
    "rust-analyzer.inlayHints.enable": true,
    "rust-analyzer.inlayHints.typeHints": false,
    "rust-analyzer.inlayHints.typeHintsSeparator": "‣ ",
    "rust-analyzer.lens.methodReferences": true,
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.updates.channel": "stable",
    "signature.enable": true,
    "tsserver.log": "verbose",
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true
    // "zig.path": "~/zls/zls"
}
